{
	"name": "corny",
	"displayName": "Corny",
	"description": "The Corniest git extension for VSCode - pull and push using action cards",
	"icon": "corny.svg",
	"version": "0.0.1",
	"publisher": "Tikal",
	"engines": {
		"vscode": "^1.24.1"
	},
	"repository": "https://github.com/tikalk/corny",
	"categories": ["Other"],
	"activationEvents": ["*"],
	"main": "./src/extension",
	"contributes": {
		"commands": [
			{
				"command": "corny.sayHello",
				"title": "Corny: Hello World"
			},
			{
				"command": "corny.gitPull",
				"title": "Corny: git pull"
			},
			{
				"command": "corny.gitPush",
				"title": "Corny: git push"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Corny",
			"properties": {
				"corny.autoshowOutputWindow": {
					"type": "boolean",
					"default": true,
					"description":
						"Automatically show and hide the output window when running commands"
				}
			}
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"dependencies": {
		"@tensorflow/tfjs": "^0.12.0",
		"@tensorflow/tfjs-node": "^0.1.7",
		"canvas": "^2.0.0-alpha.12",
		"electron": "^2.0.4",
		"lodash.maxby": "^4.6.0",
		"tree-kill": "^1.2.0"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^7.0.43",
		"eslint": "^4.11.0",
		"husky": "^0.14.3",
		"prettier": "^1.13.4",
		"pretty-quick": "^1.4.1",
		"typescript": "^2.6.1",
		"vscode": "^1.1.18"
	},
	"prettier": {
		"useTabs": true,
		"tabWidth": 4,
		"singleQuote": true,
		"trailingComma": "all",
		"printWidth": 100,
		"overrides": [
			{
				"files": "*.json",
				"options": {
					"parser": "json"
				}
			},
			{
				"files": "*.hjs",
				"options": {
					"parser": "html"
				}
			}
		]
	}
}
